{
  "hyprland/workspaces": {
        "format": "[{name} {windows} ]",
        "format-icons": {
          "1": "",
          "2": "",
          "3": "",
          "4": "",
          "5": "",
          "active": "",
          "default": ""
        },
        "format-window-separator": " ",
        "window-rewrite-default": "",
        "window-rewrite": {
          "title<.*youtube.*>": "",
          "class<firefox>": "",
          "class<google-chrome>": "",
          "class<chat-gpt>": "G",
          "class<firefox> title<.*github.*>": "",
          "kitty": "",
          "class<code-oss>": "󰨞",
          "class<org.pwmt.zathura>": "",
          "class<sioyek>": "",
          "class<org.inkscape.Inkscape>": "",
          "class<Spotify>": "󰓇",
          "class<QQ>": "󰘅",
          "class<wechat>": "",
          "class<qqmusic>": "",
          "class<Zotero>": "󰰷",
        },
        "disable-scroll": true,
        "rotate": 0,
        "all-outputs": true,
        "active-only": false,
        "on-click": "activate",
        "disable-scroll": false,
        "on-scroll-up": "hyprctl dispatch workspace -1",
        "on-scroll-down": "hyprctl dispatch workspace +1",
        "persistent-workspaces": {
        }
    },
}
